alias gs='git status'
alias gl='git pull'
alias gp='git push'
alias gc='git commit'
alias gcm='git commit -m'
alias ga='git add'
alias gd='git diff'
alias gsu='git submodule update --init --recursive'

# Use exa instead of ls
alias ls='exa'
alias ll='exa -alF'
alias la='exa -a'
alias l='exa -l'
alias lh='exa -alhF'
